{
  "name": "omnisearch-sdk",
  "version": "1.1.0",
  "description": "TypeScript SDK for web search providers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbks121/omnisearch-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/gbks121/omnisearch-sdk/issues"
  },
  "homepage": "https://github.com/gbks121/omnisearch-sdk#readme",
  "keywords": [
    "search",
    "api",
    "sdk",
    "typescript",
    "web search"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  },
  "overrides": {
    "minimatch": "^10.2.1"
  }
}
